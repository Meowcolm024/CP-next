\ottdefncosub

\begin{multicols}{3}
\begin{verbatim}
/* S-Equiv */
copy(y, x);

/* S-Bot */
y[T] = null;

/* S-Split */
var y1 = {}; // if y1 != z
var y2 = {}; // if y2 != z
J1;  J2;  J3;

/* S-Int */
y[T] = x[T];

/* S-Var */
copy(y, x);

/* S-Arrow */
y[T2] = (x1, y2) => {
  var y1 = {};  J1;
  var x2 = x[T1](y1);
  y2 = y2 || {};
  J2;  return y2;
};

/* S-All */
y[T2] = (X, y0) => {
  var x0 = x[T1](X);
  y0 = y0 || {};
  J;  return y0;
};

/* S-Rcd */
y.__defineGetter__(T2, () => {
  var x0 = x[T1];
  var y0 = {};  J;
  delete this[T];
  return this[T] = y0;
});
\end{verbatim}
\end{multicols}

