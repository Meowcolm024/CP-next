{
    "name": "zord",
    "displayName": "Zord Language Support",
    "description": "Syntax highlighting for the Zord programming language",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "publisher": "yzyzsun",
    "repository": {
        "type": "git",
        "url": "https://github.com/yzyzsun/zord.git"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "zord",
            "aliases": ["Zord", "zord"],
            "extensions": [".zord"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zord",
            "scopeName": "source.zord",
            "path": "./Zord.tmLanguage.json"
        }]
    }
}
